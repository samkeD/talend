    <elementParameter field="MEMO_JAVA" name="CODE_START" value="// start part of your Java code&#xA;System.out.println(&quot;START&quot;);&#xA;List&lt;String> names = new ArrayList&lt;>();  &#xA;List&lt;String> surnames = new ArrayList&lt;>();&#xA;List&lt;String> ages = new ArrayList&lt;>();&#xA;StringBuilder  differences = new StringBuilder(&quot;&quot;);&#xA;int count = 0;&#xA;String header = &quot;&quot;+row1;&#xA;final String FILENAME = &quot;C:\\Users\\f4839870\\Desktop\\differences_file.txt&quot;; &#xA;&#xA;"/>
    <elementParameter field="MEMO_JAVA" name="CODE_MAIN" value="// here is the main part of the component,&#xA;// a piece of code executed in the row&#xA;// loop&#xA;//System.out.println(&quot;MIDDLE&quot;);&#xA;//adds data to the relevant list&#xA; names.add(row1.Name);&#xA; surnames.add(row1.Surname);&#xA; ages.add(row1.Age);&#xA;"/>
    <elementParameter field="MEMO_JAVA" name="CODE_END" value="&#xA;        // tJavaFlex end code begins here&#xA;        // end of the component, outside/closing the loop&#xA;         System.out.println(&quot;END&quot;);&#xA;        //gets a single row of data with (column names)keys and values (column data)&#xA;        String row = header.substring(header.indexOf('[') + 1, header.lastIndexOf(']'));&#xA;        //to be used to temporarily store row string while extracting column names from the original row string&#xA;        String tempRow;&#xA;        //array to be used to store all column names&#xA;        String columns[] = new String[row.split(&quot;,&quot;).length];&#xA;        for (int i = 0; i &lt; columns.length; i++) {&#xA;            //gets single column name&#xA;            columns[i] = row.substring(0, row.indexOf('='));&#xA;            //adds column name obtained above to differences string to serve as headings in the file&#xA;            //can append source or target/destination yo columns[i] to specify where the data is coming from&#xA;            differences.append(String.format(&quot;%-10s %-10s&quot;, columns[i], columns[i]));&#xA;            tempRow = row.substring(row.indexOf(',') + 1);&#xA;            row = tempRow;&#xA;        }&#xA;        //append new line to differences so that a new row is written on a new line&#xA;        differences.append('\n');&#xA;        for (int i = 0, j = names.size() / 2; i &lt; names.size() / 2 &amp;&amp; j &lt; names.size(); i++, j++) {&#xA;            /* - the if statements will append the two differing information from the two sources to differences&#xA;               - the else statements will just append the empty string if no differences are found,&#xA;                  this is done to ensure each difference is listed under the correct column name&#xA;            */&#xA;            if (!names.get(i).equals(names.get(j))) {&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, names.get(i), names.get(j)));&#xA;                count++;&#xA;            } else&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, &quot; &quot;, &quot; &quot;));&#xA;            if (!surnames.get(i).equals(surnames.get(j))) {&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, surnames.get(i), surnames.get(j)));&#xA;                count++;&#xA;            } else&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, &quot; &quot;, &quot; &quot;));&#xA;            if (!ages.get(i).equals(ages.get(j))) {&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, ages.get(i), ages.get(j)));&#xA;                count++;&#xA;            } else&#xA;                differences.append(String.format(&quot;%-10s %-10s&quot;, &quot; &quot;, &quot; &quot;));&#xA;            //append new line to differences so that a new row is written on a new line&#xA;            differences.append('\n');&#xA;        }&#xA;        differences.append(String.format(&quot;%-10s %-10s %-10s %-10s %-10s %-10s&quot;, &quot;==========&quot;, &quot;==========&quot;, &quot;==========&quot;, &quot;==========&quot;, &quot;==========&quot;, &quot;==========&quot;));&#xA;        differences.append('\n');&#xA;        differences.append(&quot;END OF FILE&quot;);&#xA;        differences.append('\n');&#xA;        differences.append(count + &quot; DIFFERENCES WERE FOUND BETWEEN THE TWO SOURCES&quot;);&#xA;        //attempts to write the differences to a file&#xA;        if(count>0){&#xA;            try (FileWriter fileWriter = new FileWriter(FILENAME);&#xA;                 BufferedWriter bufferedWriter = new BufferedWriter(fileWriter)) {&#xA;&#xA;                bufferedWriter.write(differences.toString());&#xA;&#xA;                System.out.println(&quot;Done writing differences file...&quot;);&#xA;&#xA;            } catch (IOException e) {&#xA;                e.printStackTrace();&#xA;            }&#xA;        }&#xA;        else&#xA;            System.out.println(count +&quot; DIFFERENCES FOUND BETWEEN THE TWO SOURCES&quot;);&#xA;       "/>
